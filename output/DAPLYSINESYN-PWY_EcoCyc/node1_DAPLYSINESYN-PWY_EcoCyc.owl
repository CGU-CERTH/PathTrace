  <owl:Class rdf:about="http://www.biopax.org/release/biopax-level3.owl#EntityFeature">
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.biopax.org/release/biopax-level3.owl#Evidence"/>
    </owl:disjointWith>
    <owl:disjointWith>
